{
  "overwrite": true,
  "schema": "http://localhost:8080/graphql",
  "documents": [
    "graphql/queries/*.graphql",
    "graphql/mutations/*.graphql"
  ],
  "generates": {
    "graphql/generated/types.ts": {
      "plugins": [
        "typescript",
        "typescript-operations",
        "typescript-graphql-request"
      ]
    }
  }
}
