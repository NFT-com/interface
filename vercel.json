{
  "functions": {
    "pages/*.xml": {
      "maxDuration": 120
    },
    "pages/sitemaps/*.xml": {
      "maxDuration": 120
    },
    "pages/sitemaps/**/*.xml": {
      "maxDuration": 120
    }
  },
  "redirects": [
    {
      "source": "/whitelist",
      "destination": "https://whitelist.nft.com"
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/nftdotcom"
    },
    {
      "source": "/(y|Y)(o|O)(u|U)",
      "destination": "/app/mint-profiles"
    }
  ],
  "$schema": "https://openapi.vercel.sh/vercel.json"
}
